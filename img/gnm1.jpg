function gotop() {
	var html = $("#gototop");
	if($(window).scrollTop() <= 0) {
		$("#gototop>.top>i").hide()
	}
	$("#gototop>.top").bind("click", function() {
		if($(window).scrollTop() <= 0) {
			return false
		}
		$("body,html").animate({
			scrollTop: 0
		}, 200);
		return false
	});
	$(window).scroll(function() {
		if($(window).scrollTop() > 0) {
			$("#gototop>.top>i").fadeIn()
		} else {
			$("#gototop>.top>i").fadeOut()
		}
	})
}
$(function() {
	gotop()
});

